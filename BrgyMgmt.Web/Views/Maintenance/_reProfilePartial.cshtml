@Html.DevExpress().RichEdit(settings => {
    settings.Name = "reProfile";
    settings.CallbackRouteValues = new { Controller = "Maintenance", Action = "reProfilePartial" };
    settings.Width = System.Web.UI.WebControls.Unit.Percentage(100);
    settings.Height = 1000;
    settings.ReadOnly = false;
    settings.WorkDirectory = "Documents";
    settings.RibbonMode = RichEditRibbonMode.Ribbon;
}).Open(Server.MapPath("~/documents/runruno-profile.docx")).GetHtml()